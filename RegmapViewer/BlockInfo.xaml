<UserControl
    x:Class="RegmapViewer.BlockInfo"
    xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
    xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
    xmlns:d="http://schemas.microsoft.com/expression/blend/2008"
    xmlns:local="clr-namespace:RegmapViewer"
    xmlns:mc="http://schemas.openxmlformats.org/markup-compatibility/2006"
    d:DesignHeight="400"
    d:DesignWidth="800"
    Background="White"
    mc:Ignorable="d">
    <StackPanel>
        <Label Content="{Binding ComponentName}" />
        <Expander Header="Propertyes" IsExpanded="True">
            <DataGrid />
        </Expander>
        <Expander Header="Timming" IsExpanded="True">
            <local:BlockDelayTimingsControl />
        </Expander>
    </StackPanel>
</UserControl>
