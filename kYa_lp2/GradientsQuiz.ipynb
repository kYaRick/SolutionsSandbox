   "source": [
    "# Calculate Gx and Gy gradients\n",
    "Gx = matrix[1, 2] - matrix[1, 0]  # East - West\n",
    "Gy = matrix[2, 1] - matrix[0, 1]  # South - North\n",
    "\n",
    "# Print the Gx and Gy gradients\n",
    "print(\"Gx:\", Gx)\n",
    "print(\"Gy:\", Gy)\n",
    "print(f\"θ = {round(np.arctan2(Gy, Gx) * (180 / np.pi) % 180, 0)}\")"
   ]
