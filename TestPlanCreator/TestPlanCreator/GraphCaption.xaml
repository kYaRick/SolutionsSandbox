<UserControl x:Class="TestPlanCreator.GraphCaption"
             xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
             xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
             xmlns:mc="http://schemas.openxmlformats.org/markup-compatibility/2006" 
             xmlns:d="http://schemas.microsoft.com/expression/blend/2008" 
             xmlns:local="clr-namespace:TestPlanCreator" d:DataContext="{d:DesignInstance Type=local:GraphCaption}"
             mc:Ignorable="d" 
             d:DesignHeight="90" d:DesignWidth="800">
    <WrapPanel>
        <Label Height="40" Width="100" Content="{Binding Gen_Name}"></Label>
        <Label Height="40" Width="100" Content="{Binding TP_Name}"></Label>
        <Label Height="40" Width="100" Content="{Binding PIN_Name}"></Label>
        <Label Height="40" Width="100" Content="{Binding Signal_Name}"></Label>
    </WrapPanel>
</UserControl>
